{
  "name": "vue-polyglot",
  "version": "0.1.1",
  "description": "basic translation plugin for VueJS 2+",
  "keywords": [
    "vue",
    "vuejs",
    "polyglot",
    "vue-polyglot",
    "vuepolyglot",
    "locales",
    "i18n",
    "translation"
  ],
  "license": "MIT",
  "author": "Guillaume Vincent <guillaume@oslab.fr>",
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/polyglot.js",
  "module": "src/polyglot.js",
  "repository": "guillaumevincent/vue-polyglot",
  "scripts": {
    "precommit": "npm test",
    "prepush": "npm test",
    "build": "rm -rf dist && webpack && npm run build:min",
    "build:min": "NODE_ENV=production webpack -p",
    "prepublish": "npm run build",
    "test": "ava test --compilers js:babel-register",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "axios": "^0.15.3"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.24.0",
    "babel-runtime": "^6.23.0",
    "husky": "^0.13.3",
    "vue": "^2.2.5",
    "webpack": "^2.3.2"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "comments": false,
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
